{
    "Jwt": {
        "ExpirationHours": 1
    },

    "Serilog": {
        "MinimumLevel": {
            "Default": "Warning",
            "Override": {
                "Microsoft": "Error",
                "System": "Error"
            }
        }
    },

    "Logging": {
        "LogLevel": {
            "Default": "Warning",
            "Microsoft.AspNetCore": "Error"
        },
        "EnableSensitiveDataLogging": false
    }
}
